{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#151312",
    "light": "#151312",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Getting started",
        "pages": [
          "index",
          "quickstart"
        ]
      },
      {
        "group": "Dashboard",
        "pages": [
          "calendar",
          {
            "group": "Integrations",
            "expanded": false,
            "icon": "plug",
            "pages": [
              "integrations/google-calendar",
              "integrations/outlook-calendar",
              "integrations/apple-calendar",
              "integrations/zoom",
              "integrations/google-meet",
              "integrations/paypal"
            ]
          },
          "events",
          "availability",
          "integrations"
        ]
      },
      {
        "group": "Settings",
        "pages": [
          "settings/profile",
          "settings/calendar",
          "settings/video-conferencing"
        ]
      }
    ]
  },
  "global": {
    "anchors": [
      {
        "anchor": "Documentation",
        "href": "https://mintlify.com/docs",
        "icon": "book-open-cover"
      },
      {
        "anchor": "Blog",
        "href": "https://mintlify.com/blog",
        "icon": "newspaper"
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@novacal.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.novacal.io"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/novacalio",
      "linkedin": "https://www.linkedin.com/company/novacal-io"
    }
  },
  "styling": {
    "customCss": ["/custom.css"]
  },
  "appearance": {
    "default": "light",
    "strict": true
  }
}
